{
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
        "endOfLine":0,
      "semi": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": [
        "error", {
          "semi": false,
          "endOfLine": "auto",
          "trailingComma": "none",
          "tabWidth": 2,
          "singleQuote": false,
          "bracketSpacing": true,
          "jsxBracketSameLine": true
        }
      ]
    }
  }